{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "prisma:seed": {
      "dependsOn": [],
      "cache": false,
      "persistent": false
    },
    "prisma:generate": {
      "dependsOn": [],
      "cache": false,
      "persistent": false
    },
    "prune": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "format": {
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["$TURBO_DEFAULT$"]
    },
    "start": {
      "cache": false
    },
    "start:dev": {
      "cache": false,
      "persistent": true
    },
    "start:debug": {
      "cache": false,
      "persistent": true
    },
    "start:prod": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    },
    "test:cov": {
      "dependsOn": ["^test:cov"],
      "outputs": ["coverage/**"]
    },
    "test:debug": {
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    }
  }
}
